  "compilerOptions": {
    "target": "es2019",
    "module": "commonjs",
    "outDir": "build",
    "lib": [
      "es2019"
    ],
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "incremental": true,
    "baseUrl": "./src"
  },
  "include": [
    "**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
